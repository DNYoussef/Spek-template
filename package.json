{
  "name": "spek-enhanced-platform",
  "version": "1.0.0",
  "description": "SPEK Enhanced Development Platform with Research Intelligence and Theater Detection",
  "scripts": {
    "test": "jest",
    "test:js": "jest --coverage",
    "test:py": "python -m pytest tests/ -v --tb=short",
    "test:py:analyzer": "python -m pytest tests/test_analyzer.py -v",
    "test:py:performance": "python -m pytest tests/test_performance.py -v",
    "test:py:integration": "python -m pytest tests/test_integration.py -v",
    "test:coverage": "jest --coverage --coverageReporters=json-summary --coverageDirectory=coverage",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.{js,ts}'",
    "test:e2e": "playwright test",
    "test:e2e:ci": "playwright test --config=playwright.config.ci.js",
    "test:smoke": "node tests/smoke/smoke-tests.js",
    "lint": "eslint src/ --ext .js,.ts,.tsx && npm run lint:py",
    "lint:js": "eslint src/ --ext .js,.ts,.tsx --fix",
    "lint:py": "python -m flake8 analyzer/ --max-line-length=120 --extend-ignore=E203,W503",
    "lint:py:fix": "python -m autopep8 --in-place --recursive analyzer/",
    "typecheck": "tsc --noEmit && npm run typecheck:py",
    "typecheck:py": "python -m mypy analyzer/ --ignore-missing-imports",
    "security": "npm run security:py",
    "security:py": "python -m bandit -r analyzer/ -f json -o .claude/.artifacts/security-scan.json",
    "compliance:nasa-pot10": "node scripts/nasa-pot10-compliance.js",
    "analyze": "python test_modules.py",
    "validate": "npm run test:js && npm run test:py && npm run lint && npm run typecheck",
    "build": "tsc && npm run build:assets",
    "build:assets": "echo 'Building production assets...' && mkdir -p dist/assets",
    "start": "node dist/server.js",
    "start:dev": "ts-node src/server.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@langchain/community": "^0.3.56",
    "@langchain/core": "^0.3.77",
    "@langchain/openai": "^0.6.13",
    "@playwright/test": "^1.55.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/natural": "^5.1.5",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/typescript-estree": "^8.44.1",
    "@xstate/react": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "fs": "^0.0.1-security",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lancedb": "^0.0.1",
    "lz4": "^0.6.5",
    "natural": "^8.1.0",
    "openai": "^5.23.1",
    "playwright": "^1.55.1",
    "playwright-core": "^1.55.1",
    "ts-morph": "^27.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "xstate": "^5.22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "chokidar": "^4.0.3",
    "eslint": "^8.57.0",
    "esprima": "^4.0.1",
    "fast-check": "^4.3.0",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.11"
  },
  "engines": {
    "node": ">=16.0.0",
    "python": ">=3.8.0"
  },
  "keywords": [
    "spek",
    "analyzer",
    "connascence",
    "quality",
    "theater-detection"
  ],
  "license": "MIT"
}
